{
  "name": "replace-multiple-action",
  "version": "0.0.0",
  "private": true,
  "description": "Multiple string replace Action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts --fix",
    "lint-check": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flcdrg/replace-multiple-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "David Gardiner",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^2.0.2",
    "axios": "^1.13.2",
    "glob": "^13.0.0",
    "lru-cache": "^11.2.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.26",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.47.0",
    "@vercel/ncc": "^0.38.4",
    "eslint": "^9.39.2",
    "eslint-plugin-github": "^6.0.0",
    "eslint-plugin-jest": "^29.12.1",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.5.0",
    "jest": "^30.2.0",
    "js-yaml": "^4.1.1",
    "nerdbank-gitversioning": "^3.9.50",
    "prettier": "^3.1.1",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=20"
  }
}
